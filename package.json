{
  "name": "@dmapper/testapp",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run clean && npm run -s watch",
    "start-web": "node node_modules/nodemon/bin/nodemon --watch build/server.js ./build/server.js",
    "build": "node_modules/.bin/concurrent \"npm run build-server\" \"npm run build-client\"",
    "build-server": "WP_BACKEND=1 node node_modules/webpack/bin/webpack --colors --display-error-details --config node_modules/dm-react-webpack/index.js",
    "build-client": "WP_FRONTEND=1 node node_modules/webpack/bin/webpack --colors --display-error-details --config node_modules/dm-react-webpack/index.js",
    "watch-server": "WP_BACKEND=1 WP_WATCH=1 node node_modules/webpack/bin/webpack --watch --verbose --colors --display-error-details --config node_modules/dm-react-webpack/index.js",
    "watch-server-start": "node node_modules/just-wait -t 1000 --pattern \"build/server.js\" && npm run -s start-web",
    "watch-client": "WP_FRONTEND=1 WP_WATCH=1 node node_modules/webpack-dev-server/bin/webpack-dev-server --config node_modules/dm-react-webpack/index.js",
    "watch": "node_modules/.bin/concurrent --kill-others \"npm run watch-server-start\" \"npm run watch-server\" \"npm run watch-client\"",
    "clean": "rm -rf ./tmp",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "Private",
  "dependencies": {
    "bluebird": "^3.4.6",
    "classnames": "^2.2.5",
    "dagre": "^0.8.1",
    "dagre-layout": "^0.8.8",
    "derby-ar": "http://github.com/dmapper/derby-ar/tarball/master",
    "dm-react-webpack": "^4.2.0",
    "dm-sharedb-server": "^6.0.0",
    "express": "~4.14.0",
    "graphlib": "^2.1.5",
    "graphlibrary": "^2.2.0",
    "lodash": "^4.17.4",
    "nconf": "~0.6.8",
    "quill": "1.2.0",
    "racer": "http://github.com/dmapper/racer/tarball/other-ot-types",
    "racer-highway": "^8.0.0",
    "react": "~15.4.0",
    "react-dom": "~15.4.0",
    "react-router": "^2.8.1",
    "react-sharedb": "^0.1.3",
    "react-tap-event-plugin": "^2.0.1",
    "rich-text": "^3.1.0",
    "sharedb": "http://github.com/dmapper/sharedb/tarball/upstream-liveQuery",
    "sharedb-mongo": "http://github.com/dmapper/sharedb-mongo/tarball/aggregationSecondary",
    "sharedb-redis-pubsub": "^1.0.0-beta.1"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "chai": "~3.4",
    "concurrently": "^1.0.0",
    "just-wait": "^1.0.5",
    "mocha": "~2.3",
    "nodemon": "^1.3.8",
    "standard": "^9.0.0",
    "wdio-spec-reporter": "0.0.3",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.0",
    "xmldom": "0.1.19"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
